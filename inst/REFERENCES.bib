  booktitle = {{{LingVis}}: Visual Analytics for Linguistics},
  author = {Zhao, Jian and Chevalier, Fanny and Collins, Christopher},
  editor = {Butt, Miriam and {Hautli-Janisz}, Annette and Lyding, Verena},
  year = {2019},
  series = {{{CSLI}} Lecture Notes},
  number = {no. 220},
  pages = {29--54},
  publisher = {{CSLI Publications, Center for the Study of Language and Information}},
  address = {{Stanford, California}},
  abstract = {A discourse parser is a natural language processing system which can represent the organization of a document based on a rhetorical structure tree \textemdash{} one of the key data structures enabling applications such as text summarization, question answering and dialogue generation. Computational linguists currently rely on manually exploring and comparing the discourse structures to get intuitions for improving parsing algorithms. In this paper, we revisit our earlier work on DAViewer, an interactive visualization system for assisting computational linguists to explore, compare, evaluate, and annotate the results of discourse parsers. We present an investigation of the rationales guiding design decisions for discourse analysis and compare three alternative representations of discourse parse trees. We report the results of an expert review of these design alternatives for the task of comparing discourse parsing algorithms.},
  isbn = {978-1-68400-034-0 978-1-68400-033-3 978-1-68400-035-7},
  lccn = {QA76.9.I52 L56 2019},
  keywords = {Information visualization,LANGUAGE ARTS \& DISCIPLINES / Linguistics / General,Linguistics,Visual analytics}
}

